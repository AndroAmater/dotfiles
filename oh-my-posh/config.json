{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}} ‚ö° {{end}}{{.Folder | replace \"~\" \"üè†\"}} @ {{.HostName}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .Icon }} ",
          "foreground": "#EEFFFF",
          "background": "#444267",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "<transparent,background>\ue0b0",
          "trailing_diamond": "\ue0b0",
          "template": "{{ if .SSHSession }} \uf817 \ue0b1{{ end }} \uf007 {{ .UserName }} ",
          "foreground": "#212121",
          "background": "#5FAAE8",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07b",
            "folder_separator_icon": "<foreground,background> \ue0b1 ",
            "home_icon": "\uf015",
            "max_depth": 1,
            "style": "agnoster_full"
          },
          "leading_diamond": "<transparent,background>\ue0b0",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .Path }} ",
          "foreground": "#212121",
          "background": "#69FF94",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#EEFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#1A1A1A",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#1A1A1A{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#1A1A1A{{ end }}",
            "{{ if gt .Ahead 0 }}#1A1A1A{{ end }}",
            "{{ if gt .Behind 0 }}#1A1A1A{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "<transparent,background>\ue0b0",
          "trailing_diamond": "\ue0b0",
          "template": " \uf0aa ",
          "foreground": "#212121",
          "background": "#DC6068",
          "type": "root",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2<transparent,background>",
          "trailing_diamond": "<transparent,parentBackground>\ue0b2",
          "template": " {{ if eq .Code 0 }}\uf00c{{ else if eq .Code 130 }}\uf071 {{ .Code }}{{ else }}\uf00d {{ .Code }}{{ end }} ",
          "foreground": "#212121",
          "background": "#ABCF76",
          "type": "exit",
          "style": "diamond",
          "background_templates": [
            "{{ if eq .Code 0 }}#ABCF76{{ else if eq .Code 130 }}#C3E88D{{ else }}#F07178{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "round",
            "threshold": 0
          },
          "leading_diamond": "\ue0b2<transparent,background>",
          "trailing_diamond": "<transparent,parentBackground>\ue0b2",
          "template": " \uf254 {{ .FormattedMs }} ",
          "foreground": "#212121",
          "background": "#5FAAE8",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "leading_diamond": "\ue0b2<transparent,background>",
          "trailing_diamond": "\ue0b4",
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "foreground": "#EEFFFF",
          "background": "#444267",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ‚ùØ",
          "foreground": "#EEFFFF",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
